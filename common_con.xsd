<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:element name="abagsafe">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="adjust">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:simpleType name="align">
		<xs:restriction base="xs:string">
			<xs:enumeration value="left"/>
			<xs:enumeration value="right"/>
			<xs:enumeration value="center"/>
			<xs:enumeration value="justify"/>
			<xs:enumeration value="char"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="applic">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
			<xs:attribute name="applic-type">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="engine"/>
						<xs:enumeration value="component"/>
					</xs:restriction>
				</xs:simpleType>	
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="article-id">
		<xs:complexType>
			<xs:attribute name="id" type="xs:string" />
		</xs:complexType>
	</xs:element>

	<xs:element name="belt-rt">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="bleed">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="c-base" >
		<xs:complexType>
			<xs:attribute name="model-yr" type="xs:string" />
			<xs:attribute name="manufacturer" type="xs:string" />
			<xs:attribute name="mkt-region" type="xs:string" />
			<xs:attribute name="make" type="xs:string" />
			<xs:attribute name="model" type="xs:string" />
			<xs:attribute name="series" type="xs:string" />
			<xs:attribute name="submodel" type="xs:string" />
			<xs:attribute name="vehtype" type="vehtype" />
			<xs:attribute name="origin" type="origin" />
		</xs:complexType>
	</xs:element>

	<xs:attributeGroup name="c-refs">
		<xs:attribute name="category">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="accessories"/>
					<xs:enumeration value="body-cab"/>
					<xs:enumeration value="brakes"/>
					<xs:enumeration value="control.systems"/>
					<xs:enumeration value="driveline-axles"/>
					<xs:enumeration value="engine"/>
					<xs:enumeration value="frame-frame.accessories"/>
					<xs:enumeration value="hvac"/>
					<xs:enumeration value="restraints"/>
					<xs:enumeration value="steering"/>
					<xs:enumeration value="suspension"/>
					<xs:enumeration value="transaxle-transmission"/>
					<xs:enumeration value="align"/>
					<xs:enumeration value="geninf"/>
					<xs:enumeration value="maint"/>
					<xs:enumeration value="techinf"/>
					<xs:enumeration value="vacum"/>
					<xs:enumeration value="wiring"/>
					<xs:enumeration value="engine-performance"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="content-type" type="xs:string" />
	</xs:attributeGroup>
	
	<xs:element name="c-srvcat">
		<xs:complexType>
			<xs:attributeGroup ref="c-refs" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="c-systm">
		<xs:complexType>
			<xs:attributeGroup ref="c-refs" />
		</xs:complexType>
	</xs:element>
	
	<xs:element name="callout">
		<xs:complexType>
			<xs:choice minOccurs="1" maxOccurs="1">
				<xs:element ref="table" />
				<xs:element ref="chart" />
			</xs:choice>
		</xs:complexType>	
	</xs:element>
	
	<xs:element name="caption">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="float"/>
				<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>

	<xs:element name="category-qualifier">
		<xs:complexType>
			<xs:attributeGroup ref="symp"/>
		</xs:complexType>
	</xs:element>

	<xs:element name="caution" type="cnwicontent"/>

	<xs:element name="chart">
		<xs:complexType>
			<xs:choice>
				<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
				<xs:element ref="tgroup" minOccurs="1" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>

	<xs:element name="cmp-loc">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:group name="cnwi">
		<xs:choice>
			<xs:element ref="caution"/>
			<xs:element ref="warning"/>
			<xs:element ref="note"/>
			<xs:element ref="important"/>
		</xs:choice>
	</xs:group>

	<xs:complexType name="cnwicontent">
		<xs:choice minOccurs="1" maxOccurs="unbounded">
			<xs:element ref="para"/>
			<xs:element ref="ptxt"/>
			<xs:element ref="list"/>
			<xs:element ref="figure"/>
			<xs:element ref="table"/>
			<xs:element ref="chart"/>
			<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
		</xs:choice>
		<xs:attributeGroup ref="vdo"/>
	</xs:complexType>

	<xs:simpleType name="code.type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="title"/>
			<xs:enumeration value="posscause"/>
			<xs:enumeration value="setcondtion"/>
			<xs:enumeration value="monitorcondtion"/>
			<xs:enumeration value="cirdesc"/>
			<xs:enumeration value="test"/>
			<xs:enumeration value="diagaids"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="colspec">
		<xs:complexType>
			<xs:attribute name="colnum" type="xs:integer"/>
			<xs:attribute name="colname" type="xs:NMTOKEN"/>
			<xs:attribute name="align" type="align"/>
			<xs:attribute name="charoff" type="xs:NMTOKEN"/>
			<xs:attribute name="char" type="xs:string"/>
			<xs:attribute name="colwidth" type="xs:string"/>
			<xs:attribute name="colsep" type="xs:integer"/>
			<xs:attribute name="rowsep" type="xs:integer"/>
		</xs:complexType>
	</xs:element>

	<xs:element name="component">
		<xs:complexType mixed="true">
			<xs:attribute name="comp-name" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>

	<xs:simpleType name="component-locator">
		<xs:restriction base="xs:string">
			<xs:enumeration value="is"/>
			<xs:enumeration value="isnt"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="config">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="c-base" minOccurs="1" maxOccurs="unbounded"/>
				<xs:choice minOccurs="1" maxOccurs="1">
					<xs:element ref="c-srvcat" minOccurs="1" maxOccurs="1"/>
					<xs:element ref="c-systm" minOccurs="0" maxOccurs="unbounded"/>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="connector-id">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="date" type="xs:string"/>

	<xs:element name="def">
		<xs:complexType>
			<xs:choice minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="ptxt"/>
				<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
			</xs:choice>
			<xs:attributeGroup ref="refs"/>
		</xs:complexType>
	</xs:element>

	<xs:element name="deflist">
		<xs:complexType>
			<xs:choice minOccurs="1" maxOccurs="unbounded">
				<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
				<xs:element ref="term"/>
				<xs:element ref="def"/>
			</xs:choice>
			<xs:attributeGroup ref="refs"/>
		</xs:complexType>
	</xs:element>

	<xs:element name="desc-op">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:simpleType name="dest">
		<xs:restriction base="xs:string">
			<xs:enumeration value="info-obj"/>
			<xs:enumeration value="ftnote"/>
			<xs:enumeration value="lst-itm"/>
			<xs:enumeration value="fig"/>
			<xs:enumeration value="tbl"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="diagnose">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="diagram">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
			<xs:attributeGroup ref="refs"/>
			<xs:attribute name="diagram-type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="wiring"/>
						<xs:enumeration value="vacuum"/>
						<xs:enumeration value="hydraul"/>
						<xs:enumeration value="mechanical"/>
						<xs:enumeration value="component-location"/>
					</xs:restriction>
				</xs:simpleType>	
			</xs:attribute>
		</xs:complexType>
	</xs:element>

	<xs:element name="dis-enab">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="disassm">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="dispo">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="doc-head">
		<xs:complexType mixed="true">
			<xs:group minOccurs="0" ref="float"/>
		</xs:complexType>
	</xs:element>

	<xs:element name="doc-subhead">
		<xs:complexType mixed="true">
			<xs:group minOccurs="0" ref="float"/>
		</xs:complexType>
	</xs:element>

	<xs:simpleType name="dtc.info.type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="code.description"/>
			<xs:enumeration value="diagnostic"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="emis-app">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="emph">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
				<xs:group ref="floatnoemph"/>
			</xs:choice>
			<xs:attribute name="etype" use="required" type="etype"/>
			<xs:attribute name="color" type="xs:string"/>
		</xs:complexType>
	</xs:element>

	<xs:element name="entry">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="title" minOccurs="0" maxOccurs="1"/>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
					<xs:group ref="cnwi"/>
					<xs:element ref="para"/>
					<xs:element ref="ptxt"/>
					<xs:element ref="deflist"/>
					<xs:element ref="list"/>
					<xs:element ref="graphic"/>
					<xs:element ref="icon"/>
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="vdo"/>
			<xs:attribute name="colname" type="xs:NMTOKEN"/>
			<xs:attribute name="namest" type="xs:NMTOKEN"/>
			<xs:attribute name="nameend" type="xs:NMTOKEN"/>
			<xs:attribute name="spanname" type="xs:NMTOKEN"/>
			<xs:attribute name="morerows" type="xs:integer"/>
			<xs:attribute name="colsep" type="xs:integer"/>
			<xs:attribute name="rowsep" type="xs:integer"/>
			<xs:attribute name="rotate" type="xs:integer"/>
			<xs:attribute name="valign" type="valign"/>
			<xs:attribute name="align" type="align"/>
			<xs:attribute name="charoff" type="xs:NMTOKEN"/>
			<xs:attribute name="char" type="xs:string"/>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="IDREFS" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>

	<xs:simpleType name="enumtype">
		<xs:restriction base="xs:string">
			<xs:enumeration value="bullet"/>
			<xs:enumeration value="unlabeled"/>
			<xs:enumeration value="dash"/>
			<xs:enumeration value="arabicnum"/>
			<xs:enumeration value="loweralpha"/>
			<xs:enumeration value="upperalpha"/>
			<xs:enumeration value="lowerroman"/>
			<xs:enumeration value="upperroman"/>
			<xs:enumeration value="legal"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="etype">
		<xs:restriction base="xs:string">
			<xs:enumeration value="bold"/>
			<xs:enumeration value="boldital"/>
			<xs:enumeration value="ital"/>
			<xs:enumeration value="roman"/>
			<xs:enumeration value="underscore"/>
			<xs:enumeration value="smallcaps"/>
			<xs:enumeration value="overbar"/>
			<xs:enumeration value="overstrike"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="extxref">
		<xs:complexType mixed="true">
			<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
			<xs:attribute name="document" use="required" type="xs:string"/>
			<xs:attribute name="extrefid" type="xs:string"/>
			<xs:attribute name="filetype" type="file-type" default="sgml"/>
		</xs:complexType>
	</xs:element>

	<xs:element name="fault-isolation">
		<xs:complexType>
			<xs:choice minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="trbshoot"/>
				<xs:element ref="diagnose"/>
				<xs:element ref="testing"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="fault.code">
		<xs:complexType>
			<xs:attribute name="name" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>

	<xs:element name="figure">
		<xs:complexType>
			<xs:sequence>
				<xs:sequence>
					<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
					<xs:element ref="graphic"/>
				</xs:sequence>
				<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
				<xs:sequence>
					<xs:element ref="caption"/>
					<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
				</xs:sequence>
			</xs:sequence>
			<xs:attribute name="figtype" type="xs:string"/>
			<xs:attributeGroup ref="parms"/>
		</xs:complexType>
	</xs:element>

	<xs:simpleType name="file-type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="sgml"/>
			<xs:enumeration value="SGML"/>
			<xs:enumeration value="graphic"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:group name="float">
		<xs:choice>
			<xs:element ref="emph"/>
			<xs:element ref="ftnote"/>
			<xs:element ref="extxref"/>
			<xs:element ref="intxref"/>
			<xs:element ref="multi-ref"/>
			<xs:element ref="part-num"/>
			<xs:element ref="softextxref"/>
			<xs:element ref="sub"/>
			<xs:element ref="sup"/>
			<xs:element ref="t-code"/>
		</xs:choice>
	</xs:group>

	<xs:group name="floatnoemph">
		<xs:choice>
			<xs:element ref="ftnote"/>
			<xs:element ref="extxref"/>
			<xs:element ref="intxref"/>
			<xs:element ref="multi-ref"/>
			<xs:element ref="part-num"/>
			<xs:element ref="softextxref"/>
			<xs:element ref="sub"/>
			<xs:element ref="sup"/>
			<xs:element ref="t-code"/>
		</xs:choice>
	</xs:group>

	<xs:group name="floatnoftnote">
		<xs:choice>
			<xs:element ref="emph"/>
			<xs:element ref="extxref"/>
			<xs:element ref="intxref"/>
			<xs:element ref="multi-ref"/>
			<xs:element ref="part-num"/>
			<xs:element ref="softextxref"/>
			<xs:element ref="sub"/>
			<xs:element ref="sup"/>
			<xs:element ref="t-code"/>
		</xs:choice>
	</xs:group>

	<xs:element name="ftnote">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
				<xs:group ref="floatnoftnote"/>
			</xs:choice>
			<xs:attributeGroup ref="refs"/>
			<xs:attribute name="type" type="ftnote-type" default="table"/>
		</xs:complexType>
	</xs:element>

	<xs:simpleType name="ftnote-type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="foot"/>
			<xs:enumeration value="table"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="graphic">
		<xs:complexType>
			<xs:attribute name="hotspot" type="yn" default="n"/>
			<xs:attribute name="graphicname" type="xs:string" use="required"/>
			<xs:attribute name="oename" type="xs:string"/>
			<xs:attribute name="quick-courtesy" type="quick-courtesy"/>
			<xs:attribute name="courtesy" type="xs:string"/>
			<xs:attributeGroup ref="refs"/>
			<xs:attributeGroup ref="parms"/>
			<xs:attribute name="gwidth" type="gwidth"/>
			<xs:attribute name="gtype" type="xs:string"/>
			<xs:attribute name="gcategory" type="xs:string"/>
			<xs:attribute name="width" type="xs:string"/>
			<xs:attribute name="height" type="xs:string"/>
		</xs:complexType>
	</xs:element>

	<xs:simpleType name="gwidth">
		<xs:restriction base="xs:string">
			<xs:enumeration value="1col"/>
			<xs:enumeration value="2col"/>
			<xs:enumeration value="1landscape"/>
			<xs:enumeration value="2landscape"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="icon">
		<xs:complexType>
			<xs:sequence>
				<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
				<xs:element ref="graphic"/>
			</xs:sequence>
			<xs:attribute name="alt-text" type="xs:string"/>
		</xs:complexType>
	</xs:element>

	<xs:element name="ident">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="important" type="cnwicontent"/>

	<xs:simpleType name="index-type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="t-code"/>
			<xs:enumeration value="none"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="info-obj">
		<xs:complexType>
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="cnwi"/>
					<xs:group ref="oe.comp"/>
					<xs:group ref="oe.dtc"/>
				</xs:choice>
				<xs:element ref="title"/>
				<xs:sequence>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element ref="para"/>
						<xs:element ref="list"/>
						<xs:element ref="table"/>
						<xs:element ref="chart"/>
						<xs:element ref="figure"/>
						<xs:element ref="deflist"/>
						<xs:group ref="cnwi"/>
						<xs:group ref="oe.comp"/>
						<xs:group ref="oe.dtc"/>
					</xs:choice>
					<xs:element ref="info-obj" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:sequence>
			<!--xs:attribute name="id" type="xs:ID"/-->
			<xs:attribute name="id" type="xs:string"/>
			<xs:attribute name="content.type"/>
			<xs:attributeGroup ref="parms"/>
			<xs:attributeGroup ref="vdo"/>
		</xs:complexType>
	</xs:element>

	<xs:element name="inspect">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="intro">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="info-obj"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="intxref">
		<xs:complexType mixed="true">
			<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
			<xs:attribute name="dest" use="required" type="dest"/>
			<!--xs:attribute name="refid" use="required" type="xs:IDREF"/-->
			<xs:attribute name="refid" use="required" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="list">
		<xs:complexType>
			<xs:sequence maxOccurs="1">
				<xs:element ref="title" minOccurs="0" maxOccurs="1"/>
				<xs:choice minOccurs="1" maxOccurs="unbounded">
					<xs:group minOccurs="0" maxOccurs="unbounded" ref="oe.dtc"/>
					<xs:element ref="lst-itm"/>
					<xs:element ref="para"/>
